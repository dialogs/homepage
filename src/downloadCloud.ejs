<!DOCTYPE html>
<html lang="en">
<%- include('partials/head', { title: 'Скачать демоверсия в облаке' }) %>
<%
const links =  {
  "ios": "https://itunes.apple.com/us/app/dialog-messenger/id1134796870",
  "android": "https://play.google.com/store/apps/details?id=im.dlg.app",
  "web": "https://app.dlg.im",
  "osx": "https://cdn1.dlg.im/desktop/dialog-messenger-mac-latest.zip",
  "windows": "https://cdn1.dlg.im/desktop/dialog-messenger-win-latest.exe",
  "linux": "https://cdn1.dlg.im/desktop/dialog-messenger-linux-i386-latest.deb",
  "linux64": "https://cdn1.dlg.im/desktop/dialog-messenger-linux-amd64-latest.deb",
  "ios_ee": "https://itunes.apple.com/us/app/dialog-enterprise/id1210727252",
  "android_ee": "https://play.google.com/store/apps/details?id=im.dlg.ee",
  "web_ee": "https://ee.dlg.im",
  "osx_ee": "https://dialog-ee-desktop.s3.amazonaws.com/dialog-ee-messenger-mac-latest.zip",
  "windows_ee": "https://dialog-ee-desktop.s3.amazonaws.com/dialog-ee-messenger-win-latest.exe",
  "linux_ee": "https://dialog-ee-desktop.s3.amazonaws.com/dialog-ee-messenger-linux-i386-latest.deb",
  "linux64_ee": "https://dialog-ee-desktop.s3.amazonaws.com/dialog-ee-messenger-linux-amd64-latest.deb",
  "vk": "https://vk.com/dlgim",
  "facebook": "https://www.facebook.com/startdialog",
  "twitter": "https://twitter.com/dialoglive"
};
%>
<body>

  <div class="site__wrapper">
    <%- include('partials/header') %>

    <main class="site__middle">
      <div class="download download--cloud page-download">
        <div class="dlg-container">
          <div class="download__sections">
            <section class="download__section download__intro">
              <h1 class="download__intro-title">Демоверсия в облаке</h1>
            </section>

            <section class="download__section download__item download__desktop">
              <h2 class="download__item-title download__desktop-title">Десктопное приложение</h2>
              <div class="download__desktop-pictute">
                <img class="download__desktop-img" src="/assets/images/download/cloud-desktop.png" srcset="/assets/images/download/cloud-desktop@2x.png 2x" alt="Десктопное приложение демо в облаке">
              </div>
              <div class="download__desktop-button-box">
                <a class="button button--default" href="<%=links.osx %>">Скачать для MacOS</a>
              </div>
              <div class="download__desktop-systems">
                <div class="download__desktop-system">
                  <a class="download__item-link" href="<%=links.windows %>">Windows</a>
                </div>
                <div class="download__desktop-system">
                  <a class="download__item-link" href="<%=links.linux %>">Linux 32</a>
                </div>
                <div class="download__desktop-system">
                  <a class="download__item-link" href="<%=links.linux64 %>">Linux 64</a>
                </div>
              </div>
            </section>

            <section class="download__section download__item download__mobile">
              <h2 class="download__item-title download__mobile-title">Мобильное приложение</h2>
              <div class="download__mobile-items">
                <div class="download__mobile-item">
                  <div class="download__mobile-pictute">
                    <img class="download__mobile-img" src="/assets/images/download/cloud-mobile-iphone.png" srcset="/assets/images/download/cloud-mobile-iphone@2x.png 2x" alt="Мобильное приложение демо в облаке iPhone / iPad">
                  </div>
                  <div class="download__mobile-link-box">
                    <a class="download__item-link" href="">iPhone / iPad</a>
                  </div>
                  <div class="download__mobile-qr-box">
                    <img class="download__mobile-qr" src="/assets/images/download/cloud-mobile-iphone-qr.png" alt="">
                  </div>
                </div>
                <div class="download__mobile-item">
                  <div class="download__mobile-pictute">
                    <img class="download__mobile-img" src="/assets/images/download/cloud-mobile-android.png" srcset="/assets/images/download/cloud-mobile-android@2x.png 2x" alt="Мобильное приложение демо в облаке Android">
                  </div>
                  <div class="download__mobile-link-box">
                    <a class="download__item-link" href="">Android</a>
                  </div>
                  <div class="download__mobile-qr-box">
                    <img class="download__mobile-qr" src="/assets/images/download/cloud-mobile-android-qr.png" alt="">
                  </div>
                </div>
              </div>
            </section>

            <section class="download__section download__item download__web">
              <h2 class="download__item-title download__web-title">Web версия</h2>
              <div class="download__web-pictute">
                <img class="download__web-img" src="/assets/images/download/cloud-web.png" srcset="/assets/images/download/cloud-web@2x.png 2x" alt="Web версия демо в облаке">
              </div>
              <div class="download__web-button-box">
                <a class="button button--default" href="<%=links.web %>">Начать</a>
              </div>
            </section>
          </div>
        </div>
      </div>
    </main>

    <%- include('partials/footer') %>
  </div>

  <script src="/assets/index.js"></script>
</body>

</html>
