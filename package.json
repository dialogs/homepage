{
  "name": "@dlghq/homepage-next",
  "version": "0.0.0",
  "description": "Brand new dialog website",
  "main": "index.js",
  "scripts": {
    "server": "node server/server",
    "browsersync": "browser-sync start --proxy \"localhost:8080\" --files \"src/**/*.ejs\" \"src/**/*.html\" \"src/assets/**/*\" --port 3000",
    "webpack": "cross-env NODE_ENV=production webpack",
    "webpack:watch": "cross-env NODE_ENV=development webpack --progress --colors --watch",
    "start": "npm run server & npm run webpack:watch & npm run browsersync",
    "format": "prettier --write --loglevel warn \"src/**/*.{js,css}\"",
    "build": "npm run webpack"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "4.16.4",
    "ejs": "2.6.1",
    "flexboxgrid": "6.3.1",
    "jquery": "3.3.1",
    "normalize.css": "8.0.1"
  },
  "devDependencies": {
    "browser-sync": "2.26.3",
    "cross-env": "5.2.0",
    "css-loader": "2.1.1",
    "file-loader": "3.0.1",
    "mini-css-extract-plugin": "0.5.0",
    "postcss-loader": "3.0.0",
    "postcss-nested": "4.1.2",
    "postcss-preset-env": "6.6.0",
    "prettier": "1.16.4",
    "style-loader": "0.23.1",
    "webpack": "4.29.6",
    "webpack-cli": "3.3.0"
  }
}
